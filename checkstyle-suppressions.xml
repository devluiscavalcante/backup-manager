<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

    <suppress files=".*Test\.java" checks=".*"/>
    <suppress files=".*Tests\.java" checks=".*"/>

    <suppress files=".*Application\.java" checks="JavadocMethod"/>

    <suppress files=".*Test\.java" checks="AvoidStarImport"/>

    <suppress files=".*Application\.java" checks="MethodLength"/>
</suppressions>